<%- include('./partials/header.ejs') %>

    <div class="container">
        <h1>
            <%=strategy.title %>
        </h1>
        <div class="description"><%=strategy.description%></div> 
        <br>
        <div class="contributor">Contributor: <%=strategy.contributor%></div>
        <form action="/strategies/<%=strategy.id%>?_method=DELETE" method="POST">
            <input type="submit" value="Delete"/>
            </form>
            <form action="/strategies/<%= strategy.id %>/edit" method="GET">
                <button type="submit">Edit</button>
            </form>
                 
    </div>
    <div class="Backbutton">
        <a href="/strategies"> Back </a>
      
    </div>
  

    <%- include('./partials/footer.ejs') %>